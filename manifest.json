{
    "name": "CookiesShallNotPass",
    "description": "Blocks unwanted cookies, fully customizable",
    "version": "1.0.0",
    "manifest_version": 3,
  
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
  
    "permissions": [
      "cookies",         
      "storage",      
      "tabs"             
    ],
  
    "host_permissions": [
      "<all_urls>"   
    ] 
  
  
  }
  